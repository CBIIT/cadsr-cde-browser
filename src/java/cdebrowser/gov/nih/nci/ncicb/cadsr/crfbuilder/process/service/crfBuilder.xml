<?xml version="1.0" standalone="yes"?>
<!-- NOTE!  Adjust the following path to match your server installation... -->
<!DOCTYPE service SYSTEM "file:///z:/cora/src/dtd/Service.dtd"[
<!ENTITY docpath "crfbuilder">
<!ENTITY jsppath "/">
<!ENTITY xmlrulespath "file:///d:/apps/oracle/oc4j/j2ee/home/applications/cora/web/">
]>
<service name="CRFBuilder" class="oracle.cle.process.Service" initial="CRFBuilderRouter">
    <process name="CRFBuilderRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="15"/>
        <argument type="Boolean" value="false"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="copyTemplateForm"/>
        <infoalias local="pp2" global="copyTemplate"/>
        <infoalias local="pp3" global="deleteModule"/>
        <infoalias local="pp4" global="deleteQuestion"/>
        <infoalias local="pp5" global="deleteValidValue"/>
        <infoalias local="pp6" global="editCRFElement"/>
        <infoalias local="pp7" global="updateCRFElement"/>
        <infoalias local="pp8" global="addModuleForm"/>
        <infoalias local="pp9" global="addModule"/>
        <infoalias local="pp10" global="searchModules"/>
        <infoalias local="pp11" global="copyModules"/>
        <infoalias local="pp12" global="addQuestionForm"/>
        <infoalias local="pp13" global="addQuestion"/>
        <infoalias local="pp14" global="addValidValuesForm"/>
        <infoalias local="pp15" global="addValidValues"/>
    </process>
    <processgroup name="CreateCRFFromTemplateGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="CreateCRFFromTemplateRouter">
    <process name="CreateCRFFromTemplateRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="2"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="copyTemplateForm"/>
        <infoalias local="pp2" global="copyTemplate"/>
    </process>
    <process name="GetCopyTemplateForm" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetCopyTemplateForm" end="false">
    </process>
    <process name="CopyTemplatePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;copyTemplate.jsp"/>
    </process>
    <process name="CopyTemplate" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.CopyTemplate" end="false">
    </process>
    <process name="DisplayCRFPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;crfBrowse.jsp"/>
    </process>
    <process name="BuildCRFTree" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.BuildCRFTree" end="false">
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;failurePage.jsp"/>
    </process>
<transition src="CreateCRFFromTemplateRouter" condition="tc1" dest="GetCopyTemplateForm"/>
<transition src="CreateCRFFromTemplateRouter" condition="tc2" dest="CopyTemplate"/>
<transition src="GetCopyTemplateForm" condition="success" dest="CopyTemplatePage"/>
<transition src="GetCopyTemplateForm" condition="failure" dest="FailurePage"/>
<transition src="CopyTemplate" condition="success" dest="DisplayCRFPage"/>
<transition src="CopyTemplate" condition="failure" dest="FailurePage"/>
<transition src="BuildCRFTree" condition="success" dest="DisplayCRFPage"/>
<transition src="BuildCRFTree" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="MaintainCRFElementsGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="MaintainCRFElementsRouter">
    <process name="MaintainCRFElementsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="5"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="deleteModule"/>
        <infoalias local="pp2" global="deleteQuestion"/>
        <infoalias local="pp3" global="deleteValidValue"/>
        <infoalias local="pp4" global="editCRFElement"/>
        <infoalias local="pp5" global="updateCRFElement"/>
    </process>
    <process name="DeleteModule" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.DeleteModule" end="false">
    </process>
    <process name="BuildCRFTree" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.BuildCRFTree" end="false">
    </process>
    <process name="DeleteQuestion" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.DeleteQuestion" end="false">
    </process>
    <process name="DeleteValidValue" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.DeleteValidValue" end="false">
    </process>
    <process name="CRFDisplayPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;crfBrowse.jsp"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;failurePage.jsp"/>
    </process>
    <process name="GetCRFElement" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetCRFElement" end="false">
    </process>
    <process name="EditCRFElementPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;editCRFElement.jsp"/>
    </process>
    <process name="UpdateCRFElement" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.UpdateCRFElement" end="false">
    </process>
<transition src="MaintainCRFElementsRouter" condition="tc1" dest="DeleteModule"/>
<transition src="MaintainCRFElementsRouter" condition="tc2" dest="DeleteQuestion"/>
<transition src="MaintainCRFElementsRouter" condition="tc3" dest="DeleteValidValue"/>
<transition src="MaintainCRFElementsRouter" condition="tc4" dest="GetCRFElement"/>
<transition src="MaintainCRFElementsRouter" condition="tc5" dest="UpdateCRFElement"/>
<transition src="DeleteModule" condition="success" dest="BuildCRFTree"/>
<transition src="BuildCRFTree" condition="failure" dest="FailurePage"/>
<transition src="BuildCRFTree" condition="success" dest="CRFDisplayPage"/>
<transition src="DeleteQuestion" condition="success" dest="BuildCRFTree"/>
<transition src="DeleteValidValue" condition="success" dest="BuildCRFTree"/>
<transition src="GetCRFElement" condition="success" dest="EditCRFElementPage"/>
<transition src="GetCRFElement" condition="failure" dest="FailurePage"/>
<transition src="UpdateCRFElement" condition="success" dest="GetCRFElement"/>
    </processgroup>
    <processgroup name="AddCRFElementsGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="AddCRFElementsRouter">
    <process name="AddCRFElementsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="8"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="addModuleForm"/>
        <infoalias local="pp2" global="addModule"/>
        <infoalias local="pp3" global="searchModules"/>
        <infoalias local="pp4" global="copyModules"/>
        <infoalias local="pp5" global="addQuestionForm"/>
        <infoalias local="pp6" global="addQuestion"/>
        <infoalias local="pp7" global="addValidValuesForm"/>
        <infoalias local="pp8" global="addValidValues"/>
    </process>
    <process name="GetCreateModuleForm" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetModuleCreateForm" end="false">
    </process>
    <process name="CreateNewModule" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.CreateModule" end="false">
    </process>
    <process name="CreateNewModulePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;createModule.jsp"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;failurePage.jsp"/>
    </process>
    <process name="GetModules" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetModules" end="false">
    </process>
    <process name="ModuleSearchPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;searchModule.jsp"/>
    </process>
    <process name="CopyModules" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.CopyModules" end="false">
    </process>
    <process name="ModuleSuccessPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;moduleSuccess.jsp"/>
    </process>
    <process name="GetCreateQuestionForm" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetQuestionCreateForm" end="false">
    </process>
    <process name="CreateQuestionPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;createQuestion.jsp"/>
    </process>
    <process name="CreateNewQuestion" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.CreateQuestion" end="false">
    </process>
    <process name="QuestionSuccessPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;questionSuccess.jsp"/>
    </process>
    <process name="GetValidValuesCreateForm" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.GetValidValuesCreateForm" end="false">
    </process>
    <process name="CreateNewValidValuesPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;createValidValue.jsp"/>
    </process>
    <process name="AddValidValues" class="gov.nih.nci.ncicb.cadsr.crfbuilder.process.CreateValidValues" end="false">
    </process>
    <process name="ValidValuesSuccessPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;validValueSuccess.jsp"/>
    </process>
<transition src="AddCRFElementsRouter" condition="tc2" dest="CreateNewModule"/>
<transition src="AddCRFElementsRouter" condition="tc1" dest="GetCreateModuleForm"/>
<transition src="AddCRFElementsRouter" condition="tc3" dest="GetModules"/>
<transition src="AddCRFElementsRouter" condition="tc4" dest="CopyModules"/>
<transition src="AddCRFElementsRouter" condition="tc5" dest="GetCreateQuestionForm"/>
<transition src="AddCRFElementsRouter" condition="tc6" dest="CreateNewQuestion"/>
<transition src="AddCRFElementsRouter" condition="tc7" dest="GetValidValuesCreateForm"/>
<transition src="AddCRFElementsRouter" condition="tc8" dest="AddValidValues"/>
<transition src="GetCreateModuleForm" condition="success" dest="CreateNewModulePage"/>
<transition src="GetCreateModuleForm" condition="failure" dest="FailurePage"/>
<transition src="CreateNewModule" condition="success" dest="CreateNewModulePage"/>
<transition src="GetModules" condition="success" dest="ModuleSearchPage"/>
<transition src="GetModules" condition="failure" dest="FailurePage"/>
<transition src="CopyModules" condition="success" dest="ModuleSuccessPage"/>
<transition src="CopyModules" condition="failure" dest="FailurePage"/>
<transition src="GetCreateQuestionForm" condition="success" dest="CreateQuestionPage"/>
<transition src="GetCreateQuestionForm" condition="failure" dest="FailurePage"/>
<transition src="CreateNewQuestion" condition="success" dest="QuestionSuccessPage"/>
<transition src="CreateNewQuestion" condition="failure" dest="CreateQuestionPage"/>
<transition src="GetValidValuesCreateForm" condition="success" dest="CreateNewValidValuesPage"/>
<transition src="GetValidValuesCreateForm" condition="failure" dest="FailurePage"/>
<transition src="AddValidValues" condition="failure" dest="CreateNewValidValuesPage"/>
<transition src="AddValidValues" condition="success" dest="ValidValuesSuccessPage"/>
    </processgroup>
<transition src="CRFBuilderRouter" condition="tc2" dest="CreateCRFFromTemplateGroup"/>
<transition src="CRFBuilderRouter" condition="tc1" dest="CreateCRFFromTemplateGroup"/>
<transition src="CRFBuilderRouter" condition="tc3" dest="MaintainCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc5" dest="MaintainCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc4" dest="MaintainCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc6" dest="MaintainCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc7" dest="MaintainCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc8" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc9" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc10" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc11" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc12" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc13" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc14" dest="AddCRFElementsGroup"/>
<transition src="CRFBuilderRouter" condition="tc15" dest="AddCRFElementsGroup"/>
<grouptransition srcgroup="CreateCRFFromTemplateGroup" srcprocess="CopyTemplatePage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="CreateCRFFromTemplateGroup" srcprocess="DisplayCRFPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="CreateCRFFromTemplateGroup" srcprocess="FailurePage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="MaintainCRFElementsGroup" srcprocess="CRFDisplayPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="MaintainCRFElementsGroup" srcprocess="FailurePage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="MaintainCRFElementsGroup" srcprocess="EditCRFElementPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="CreateNewModulePage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="FailurePage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="ModuleSearchPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="ModuleSuccessPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="CreateQuestionPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="QuestionSuccessPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="CreateNewValidValuesPage" dest="CRFBuilderRouter"/>
<grouptransition srcgroup="AddCRFElementsGroup" srcprocess="ValidValuesSuccessPage" dest="CRFBuilderRouter"/>
</service>