<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="gov.nih.nci.ncicb.cadsr.common.persistence.hibernate">
    <class name="DENames" table="DATA_ELEMENTS" dynamic-update="true">
        <meta attribute="implement-equals">true</meta>    
        <cache usage="read-only"/>
        
        <id name="id" type="string" unsaved-value="null" >
            <column name="DE_IDSEQ" not-null="true"/>
            <generator class="increment"/>
        </id>
        
        <property column="long_name" name="longName" type="string"/>
        <property column="CDE_ID" name="publicId" type="string"/>
        <property column="VERSION" name="version" type="string"/>
        
    </class>
</hibernate-mapping>
