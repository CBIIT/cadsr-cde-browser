<?xml version="1.0"?>
<!DOCTYPE cle-providers SYSTEM "http://xmlns.oracle.com/ias/mvc/cle-providers.dtd">
<cle-providers>
  <provider name="cdebrowser_bc4j" class="oracle.clex.persistence.bc4j.ApplicationModuleProvider">
    <property name="user" value="@database.user@"/>
    <property name="password" value="@database.password@"/>
    <property name="modulename" value="gov.nih.nci.ncicb.cadsr.common.persistence.bc4j.CDEBrowserBc4jModule"/>
    <property name="connectionstring" value="@database.url@"/>
    <property name="host" value="@database.server@"/>
    <property name="sid" value="@database.name@"/>
    <property name="persistencebase" value="gov.nih.nci.ncicb.cadsr.common.persistence.bc4j"/>
    <property name="failover" value=""/>
  </provider>
</cle-providers>