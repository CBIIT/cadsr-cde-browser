<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
   <form-bean name="displayCartForm"
      type="gov.nih.nci.ncicb.cadsr.common.struts.formbeans.CDECartFormBean"/>
    <form-bean name="changeAssociationForm"
      type="gov.nih.nci.ncicb.cadsr.common.struts.formbeans.GenericDynaFormBean"
      dynamic="true">
      <form-property name="questionIndex" type="java.lang.String"/>
      <form-property name="moduleIndex" type="java.lang.Integer"/>
      <form-property name="selectedText" type="java.lang.String"/>
    </form-bean>
  </form-beans>
  <global-exceptions>
    <exception type="gov.nih.nci.ncicb.cadsr.common.exception.FatalException"
      scope="request" path="/common/systemError.jsp"
      key="error.system.message"/>
    <exception type="gov.nih.nci.ncicb.cadsr.common.exception.InvalidUserException"
      scope="request" path="/cdeBrowse.jsp"
      key="error.system.message"/>      
  </global-exceptions>
  <global-forwards>
    <forward name="cdebrowserHome" path="/cdeBrowse.jsp?PageId=DataElementsGroup" redirect="true"/>
    <forward name="printerVersion" path="/formbuilder/printerVersion.jsp"
      redirect="true"/>
    <forward name="framedSearchResultsPage" path="/formbuilder/formbuilder.jsp"
      redirect="false"/>
  </global-forwards>
  <action-mappings>
    
    <action name="displayCartForm" path="/cdeCartAction" parameter="method"
      scope="session"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CDECartAction"
      validate="false">
      <forward name="success" path="/cdebrowser/displayCDECart.jsp"/>
      <forward name="saveSuccess" path="/formCDECartRedirectAction.do?method=addItems" redirect="true"/>
    </action>
    <action name="displayCartForm" path="/formCDECartRedirectAction" parameter="method"
      scope="session"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.SecureCDECartAction"
      validate="false">
      <forward name="addDeleteSuccess" path="/formCDECartAction.do?method=displayCDECart"/>
    </action>    
    <action name="displayCartForm" path="/formCDECartAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.SecureCDECartAction"
      validate="false">
      <forward name="success" path="/formbuilder/displayCDECart.jsp"/>
      <forward name="addDeleteSuccess" path="/formCDECartAction.do?method=displayCDECart"/>
    </action>   
  </action-mappings>
  <message-resources parameter="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.common.ApplicationResources"/>
 
  <plug-in className="gov.nih.nci.ncicb.cadsr.common.struts.common.SpringWebContextPlugIn">  
  </plug-in> 
  
  <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
    <set-property 
      property="pathnames" 
      value="/WEB-INF/formbuilder/validator-rules.xml, 
             /WEB-INF/formbuilder/validation.xml"/>
  </plug-in>


</struts-config>
