<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="moduleRepeatForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="moduleIndex" type="java.lang.Integer"/>
      <form-property name="numberOfModuleRepetitions" type="java.lang.Integer"/>      
      <form-property name="questionDefaults" type="java.lang.String[]"/> 
    </form-bean>
      
  </form-beans>

  <action-mappings>
    <action name="formEditForm" path="/saveFormModuleRepeatAction" parameter="method"
      scope="session"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.FormEditAction"
      validate="false">      
      <forward name="failure" path="/formbuilder/formEdit.jsp" redirect="false"/>  
      <forward name="noChanges" path="/moduleRepeatAction.do?method=getModuleToRepeat" redirect="false"/>
      <forward name="saveConfirm" path="/formbuilder/formSaveConfirmModuleRepitition.jsp" redirect="false"/>
      <forward name="sucess" path="/moduleRepeatAction.do?method=getToModuleToRepeat" redirect="false"/>
    </action>   
    
    <action name="moduleRepeatForm" path="/moduleRepeatAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.ModuleRepetitionAction"
      validate="false">      
      <forward name="viewRepetitions" path="/formbuilder/manageModuleRepetition.jsp" redirect="false"/>
      <forward name="done" path="/formbuilder/formEdit.jsp" redirect="false"/>
    </action>  
  </action-mappings>

</struts-config>
