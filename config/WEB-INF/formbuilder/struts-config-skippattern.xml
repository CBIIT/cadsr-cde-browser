<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="publishForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="formIdSeq" type="java.lang.String"/>
    </form-bean>
  </form-beans>
  
  <action-mappings>
    <action name="publishForm" path="/formbuilder/skipAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.SkipPatternAction"
      validate="false"
      >
      <forward name="success" path="/formbuilder/addSkipPattern.jsp"/>
      <forward name="failure" path="/formDetailsAction.do?method=getFormDetails"/>
    </action>
  </action-mappings>

</struts-config>
