<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="skipForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="skipInstruction" type="java.lang.String"/>

      <form-property name="formIdSeq" type="java.lang.String"/>
      <form-property name="moduleIndex" type="java.lang.String"/>
      <form-property name="questionIndex" type="java.lang.String"/>
      <form-property name="validValueIndex" type="java.lang.String"/>
    </form-bean>

  </form-beans>
  
  <action-mappings>
    <action name="skipForm" path="/formbuilder/skipAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.SkipPatternAction"
      validate="false">
      
      <forward name="editSkipPattern" path="/formbuilder/skipPatternInfoEdit.jsp"/>    
      <forward name="createSkipPattern" path="/formbuilder/skipPatternInfoCreate.jsp"/>  
      <forward name="skipToFormLocation" path="/formbuilder/skipToFormDetails.jsp"/>
      <forward name="skipToFormSearch" path="/formSearchAction.do?method=getAllForms"
        redirect="false"/>
     <forward name="backToFormEdit" path="/formToEditAction.do?method=getFormToEdit"
       redirect="false"/>
    
     <forward name="backToModuleEdit" path="/moduleToEditAction.do?method=getModuleToEdit"
      redirect="false"/>    
    
    </action>
    
  </action-mappings>

</struts-config>
