<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="ocrform"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.formbeans.UmlBrowserBaseDynaFormBean"
      dynamic="true">
      <form-property name="objectClassIdseq" type="java.lang.String"/>
      <form-property name="ocrIndex" type="java.lang.String"/>
      <form-property name="ocrDirection" type="java.lang.String"/>     
    </form-bean>
  </form-beans>
  <form-beans>
    <form-bean name="ocform"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.formbeans.UmlBrowserBaseDynaFormBean"
      dynamic="true">
      <form-property name="objectClassIdseq" type="java.lang.String"/>  
      <form-property name="resetCrumbs" type="java.lang.String"/>        
    </form-bean>
  </form-beans>  
  
  <action-mappings>
    <action name="ocrform" path="/umlbrowser/ocrDetailsAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.actions.ObjectClassRelationshipAction"
      validate="false"
      >
      <forward name="success" path="/umlbrowser/ocrDetails.jsp"/>
      <forward name="failure" path="/common/systemError.jsp"/>
    </action>
    
    <action name="ocrform" path="/umlbrowser/navigateOCRAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.actions.ObjectClassRelationshipAction"
      validate="false"
      >
      <forward name="success" path="/umlbrowser/ocrDetails.jsp"/>
      <forward name="failure" path="/common/systemError.jsp"/>
    </action>    

    <action name="ocrform" path="/umlbrowser/clearNavigationPathAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.actions.ObjectClassRelationshipAction"
      validate="false"
      >
      <forward name="success" path="/umlbrowser/ocrDetails.jsp"/>
    </action>  
    
  </action-mappings>
  
  <action-mappings>
    <action name="ocform" path="/umlbrowser/ocDetailsAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.umlbrowser.struts.actions.ObjectClassAction"
      validate="false"
      >
      <forward name="success" path="/umlbrowser/ocDetails.jsp"/>
      <forward name="failure" path="/common/systemError.jsp"/>
    </action>
      

  </action-mappings>  
  
  <plug-in className="gov.nih.nci.ncicb.cadsr.struts.common.ObjectPlugIn">
    <set-property 
      property="applicationKey" 
      value="ApplicationServiceLocatorClassName"/>
    <set-property 
      property="className" 
      value="gov.nih.nci.ncicb.cadsr.servicelocator.spring.ApplicationServiceLocatorImpl"/>      
  </plug-in>
  <plug-in className="gov.nih.nci.ncicb.cadsr.struts.common.SpringWebContextPlugIn">
    
  </plug-in>  
  
  
</struts-config>
