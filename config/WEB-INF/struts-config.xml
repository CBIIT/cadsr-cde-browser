<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
   <form-beans>
      <form-bean name="searchForm" type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean" dynamic="true">
         <form-property name="context" type="java.lang.String"/>
         <form-property name="method" type="java.lang.String"/>
      </form-bean>
   </form-beans>
   <action-mappings>
      <action path="/formAction" type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.FormAction" parameter="method" name="searchForm" scope="request" validate="false">
         <forward path="/formbuilder/form_search.jsp" name="success"/>
      </action>
   </action-mappings>
   <controller locale="true"/>
   <message-resources parameter="gov.nih.nci.ncicb.cadsr.formbuilder.struts.common.ApplicationResources"
    null="false"/>
   <plug-in className="gov.nih.nci.ncicb.cadsr.formbuilder.service.ServiceDelegateFactory"/>
</struts-config>
