<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="serachPrefForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="src" type="java.lang.String"/> 
      <form-property name="moduleIndex" type="java.lang.String"/>
      <form-property name="questionIndex" type="java.lang.String"/>
      <form-property name="excludeTestContext" type="java.lang.String"/>

    </form-bean>
</form-beans>
  <action-mappings>
    <action name="serachPrefForm" path="/cdebrowser/cdeSearchPrefAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CDESearchPrefAction"
      validate="false">
       <forward name="searchPage" path="/cdeBrowse.jsp?PageId=DataElementsGroup"/>
       <forward name="prefPage" path="/cdebrowser/cdeSearchPreferences.jsp"/>         
    </action>
            
   </action-mappings>

</struts-config>
