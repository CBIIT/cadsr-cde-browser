<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
  <form-beans>
    <form-bean name="screenChangeCDEForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="browserSearchScreenType" type="java.lang.String"/>
      <form-property name="baseQuery" type="java.lang.String"/>
    </form-bean>
</form-beans>
  <action-mappings>
    <action name="screenChangeCDEForm" path="/cdebrowser/screenTypeAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.ScreenTypeAction"
      validate="false">
      <forward name="success" path="/cdebrowser/dataElementsSearch.jsp"/>
      <forward name="failure" path="/cdebrowser/dataElementsSearch.jsp"/>       
    </action>
    
   </action-mappings>

</struts-config>
