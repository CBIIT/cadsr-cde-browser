<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="compareCDEForm"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.formbeans.FormBuilderBaseDynaFormBean"
      dynamic="true">
      <form-property name="selectDE" type="java.lang.String[]"/>
      <form-property name="cdeCompareDisplayOrder" type="java.lang.String[]"/>
      <form-property name="cdeToRemove" type="java.lang.String[]"/> 
    </form-bean>
</form-beans>
  <action-mappings>
    <action name="compareCDEForm" path="/cdebrowser/compareCDEAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">
      <forward name="success" path="/cdebrowser/compareCDEs.jsp"/>
      <forward name="search" path="/cdebrowser/dataElementsSearch.jsp"/>    
      <forward name="failure" path="/cdebrowser/cdeCompareFailure.jsp"/> 
    </action>
    
    <action name="compareCDEForm" path="/cdebrowser/CDECompareExcelDownload" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">
    </action>
    
    <action name="compareCDEForm" path="/cdebrowser/changeCompareOrder" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">      
      <forward name="success" path="/cdebrowser/compareCDEs.jsp"/>
      <forward name="failure" path="/cdebrowser/compareCDEs.jsp"/> 
    </action>
    
    <action name="compareCDEForm" path="/cdebrowser/addToCompareListAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">      
      <forward name="success" path="/cdebrowser/dataElementsSearch.jsp"/>
      <forward name="failure" path="/cdebrowser/dataElementsSearch.jsp"/> 
    </action>

    <action name="compareCDEForm" path="/cdebrowser/removeFromCompareListAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">      
      <forward name="success" path="/cdebrowser/compareCDEs.jsp"/>
      <forward name="failure" path="/cdebrowser/compareCDEs.jsp"/> 
      <forward name="lessThanTwo" path="/cdebrowser/cdeCompareFailure.jsp"/> 
    </action>    
 
     <action name="compareCDEForm" path="/cdebrowser/doneCompareListAction" parameter="method"
       scope="request"
       type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
       validate="false">      
       <forward name="success" path="/cdeBrowse.jsp?PageId=DataElementsGroup"/>
       <forward name="failure" path="/cdeBrowse.jsp?PageId=DataElementsGroup"/> 
    </action>  
    
   </action-mappings>

</struts-config>
