<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="compareCDEForm"
      type="org.apache.struts.validator.DynaValidatorForm"
      dynamic="true">
      <form-property name="dataElementDetails" type="java.util.Collection"/>
    </form-bean>
</form-beans>
  <action-mappings>
    <action name="compareCDEForm" path="/cdebrowser/compareCDEAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.cdebrowser.struts.actions.CompareCDEAction"
      validate="false">
      <forward name="success" path="/cdebrowser/compareCDEs.jsp"/>
    </action>
  
   </action-mappings>

</struts-config>
