<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <session>
      <description>Session Bean ( Stateless )</description>
      <display-name>FormBuilder</display-name>
      <ejb-name>FormBuilder</ejb-name>
      <home>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilderHome</home>
      <remote>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilder</remote>
      <local-home>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilderLocalHome</local-home>
      <local>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilderLocal</local>
      <ejb-class>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.impl.FormBuilderEJB</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
      <env-entry>
        <description>Class name of the DAOFactory that FormBuilderEJB will use.</description>
        <env-entry-name>DAOFactoryClassName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>gov.nih.nci.ncicb.cadsr.persistence.dao.jdbc.JDBCDAOFactory</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>DriverManagerDS</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>connectionString</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>jdbc:oracle:thin:@localhost:1521:red</env-entry-value>
      </env-entry>      
      <env-entry>
        <env-entry-name>driverClassName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>oracle.jdbc.driver.OracleDriver</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>username</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>scott</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>tiger</env-entry-value>
      </env-entry>      
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>FormBuilder</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
