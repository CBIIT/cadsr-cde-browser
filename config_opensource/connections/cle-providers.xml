<?xml version="1.0"?>
<!DOCTYPE cle-providers SYSTEM "http://xmlns.oracle.com/ias/mvc/cle-providers.dtd">
<cle-providers>
  <provider name="cdebrowser_bc4j" class="oracle.clex.persistence.bc4j.ApplicationModuleProvider">
    <property name="user" value="username"/>
    <property name="password" value="password"/>
    <property name="modulename" value="gov.nih.nci.ncicb.cadsr.persistence.bc4j.CDEBrowserBc4jModule"/>
    <property name="connectionstring" value="jdbc:oracle:thin:username/password@hostname:port:sid"/>
    <property name="host" value="hostname"/>
    <property name="sid" value="sid"/>
    <property name="persistencebase" value="gov.nih.nci.ncicb.cadsr.persistence.bc4j"/>
    <property name="failover" value=""/>
  </provider>
</cle-providers>